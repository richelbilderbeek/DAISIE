    "contents" : "DAISIE_IC <- function(datalist,initparsopt,idparsopt,parsfix,idparsfix,endmc = 1000,res = 100,cond = 0,ddmodel = 0)\n{\n  MLE_obs <- DAISIE_ML(\n     datalist = datalist,\n     initparsopt = initparsopt,\n     idparsopt = idparsopt,\n     parsfix = parsfix,\n     idparsfix = idparsfix,\n     idparsnoshift = 6:10,\n     res = res,\n     ddmodel = ddmodel,\n     cond = cond,\n     eqmodel = 0,\n     x_E = 0.95,\n     x_I = 0.98,\n     tol = c(1e-04, 1e-05, 1e-07),\n     maxiter = 1000 * round((1.25)^length(idparsopt)),\n     methode = \"lsodes\",\n     optimmethod = 'subplex'\n     )\n  sims <- DAISIE_sim(\n     time = datalist$island_age,\n     M = datalist$not_present,         #add the number of species that are present\n     pars = MLE_obs[1:5],\n     replicates = endmc,\n     format = TRUE,\n     sample_freq = 1,\n     plot_sims = FALSE\n     )\n  MLE <- rep(0,endmc)\n  LL <- rep(0,endmc)\n  for(mc in 1:endmc)\n  {\n    MLE[[mc]] <- DAISIE_ML(\n       datalist = sims[[mc]],\n       initparsopt = MLE_obs[idparsopt],\n       idparsopt = idparsopt,\n       parsfix = MLE_obs[parsfix],\n       idparsfix = idparsfix,\n       idparsnoshift = 6:10,\n       res = res,\n       ddmodel = ddmodel,\n       cond = cond,\n       eqmodel = 0,\n       x_E = 0.95,\n       x_I = 0.98,\n       tol = c(1e-04, 1e-05, 1e-07),\n       maxiter = 1000 * round((1.25)^length(idparsopt)),\n       methode = \"lsodes\",\n       optimmethod = 'subplex'\n       )$loglik\n    LL[[mc]] <- DAISIE_loglik_all(\n       pars1 = MLE[[mc]][1:5],\n       pars2 = c(res,ddmodel,cond,0),\n       datalist = datalist,\n       methode = \"lsodes\"\n       )\n  }\n  WIC <- -2 * MLE_obs$loglik - 2 * mean(LL - MLE)\n  AICb <- -2 * MLE_obs$loglik - 4 * mean(LL - MLE_obs)\n  out <- list(WIC = WIC,AICb = AICb)\n  return(out)\n}",
